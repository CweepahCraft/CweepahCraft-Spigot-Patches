Subject: [PATCH 13/13] Set correct flag for loot pickup on NBT load

---
 src/main/java/net/minecraft/server/EntityInsentient.java | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/src/main/java/net/minecraft/server/EntityInsentient.java b/src/main/java/net/minecraft/server/EntityInsentient.java
index 8c35b22..48ff4c8 100644
--- a/src/main/java/net/minecraft/server/EntityInsentient.java
+++ b/src/main/java/net/minecraft/server/EntityInsentient.java
@@ -388,7 +388,7 @@ public abstract class EntityInsentient extends EntityLiving {
         if (nbttagcompound.hasKeyOfType("CanPickUpLoot", 1)) {
             boolean data = nbttagcompound.getBoolean("CanPickUpLoot");
             if (isLevelAtLeast(nbttagcompound, 1) || data) {
-                this.l(data);
+                this.m(data);
             }
         }
 
